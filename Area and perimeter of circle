class circle():
    def __init__(self,radius):
        self.radius=radius
    def area(self):
        return 3.14*(self.radius**2)
    def perimeter(self):
        return 2*3.14*self.radius
a=int(input("Enter radius of circle: "))
obj=circle(a)
print("Area of circle:",obj.area())
print("Perimeter of circle:",obj.perimeter())
